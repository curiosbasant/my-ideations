{
  "name": "@my/db",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "tsc -P tsconfig.lint.json",
    "push": "pnpm with-env -- drizzle-kit push && pnpm with-env -- tsx src/migrate.ts",
    "push:prod": "pnpm with-env:prod -- drizzle-kit push && pnpm with-env:prod -- tsx src/migrate.ts",
    "reset": "supabase db reset && pnpm push",
    "seed": "pnpm with-env -- tsx src/seed.ts",
    "start": "supabase start",
    "status": "supabase status",
    "stop": "supabase stop",
    "studio": "drizzle-kit studio",
    "with-env": "dotenv -e ../../.env.local",
    "with-env:prod": "dotenv -e ../../.env.production.local"
  },
  "dependencies": {
    "@my/lib": "workspace:*",
    "drizzle-orm": "^0.44.4",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@my/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "0.3.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
