{
  "name": "@my/db",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./functions": "./src/utils/pg-functions.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "tsc --noEmit",
    "push": "pnpm with-env -- drizzle-kit push",
    "push:prod": "pnpm with-env:prod -- drizzle-kit push",
    "migrate": "pnpm with-env -- tsx src/migrate.ts",
    "reset": "supabase db reset && pnpm push",
    "seed": "pnpm with-env -- tsx src/seed.ts",
    "start": "supabase start",
    "status": "supabase status",
    "stop": "supabase stop",
    "studio": "drizzle-kit studio",
    "with-env": "dotenv -e ../../.env.local",
    "with-env:prod": "dotenv -e ../../.env.production.local"
  },
  "dependencies": {
    "@my/lib": "workspace:*",
    "drizzle-orm": "1.0.0-beta.15-859cf75",
    "drizzle-plus": "1.0.0-beta.34",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@my/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "drizzle-seed": "0.3.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
